<!DOCTYPE html>
 
<html lang="en" class="no-js">
<head>
    <meta http-equiv="content-type" content="text/html;charset=utf-8" />
    <!--[if IE]>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <![endif]-->
    <title>@ViewData["Title"]</title>
    @RenderSection("additionalMeta", false)
    <meta name="description" content="Information and guidance on promoting sustainable forest management. Understanding PEFC chain of custody certification for timber, wood based products, paper, packaging and print media. PEFC label generator and UK news and events" />
    <link rel="canonical" href="index.html" />
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" media="screen" href="~/themes/ChungChiRung/assets/style-main.css" />
    <!-- asynchronous google analytics snippet -->
    <!-- End Google Analytics -->
    <script src="~/themes/ChungChiRung/assets/application.js"></script>
    <script src="~/themes/ChungChiRung/assets/modernizr-min.js"></script>
    @RenderSection("head", required: false)
</head>
<body id="home-page">
    <div id="fb-root"></div>
    <script async defer crossorigin="anonymous" src="https://connect.facebook.net/vi_VN/sdk.js#xfbml=1&version=v7.0&appId=349559452627839&autoLogAppEvents=1" nonce="ulKvYhwg"></script>

    <div class="container">
        <div class="hidden-xs"></div>
        <div class="row">
            <header class="col-xs-12" id="header">
                <div class="header-widget row">
                    <div class="col-sm-2 col">
                        <div class="hidden-xs">
                            <a href="/"><img alt="PEFC" height= "137"; src="~/themes/ChungChiRung/assets/logo.png" /></a>
                        </div>
                        <div class="visible-xs">
                            <div class="text-center"><a href="/"><img alt="PEFC" width="137" src="~/themes/ChungChiRung/assets/logo.png" /></a></div>
                        </div>
                    </div>
                    <div class="col-sm-6 tagline col">
                        <div class="text-container">
                            <div class="hidden-xs">
                                <h3>VĂN PHÒNG CHỨNG CHỈ RỪNG VIỆT NAM</h3>
                                <h4>VIETNAM FOREST CERTIFICATION OFFICE</h4>
                            </div>
                            <div class="visible-xs">
                                <div class="text-center tagline-small">
                                    <h3>VĂN PHÒNG CHỨNG CHỈ RỪNG VIỆT NAM</h3>
                                    <h4>VIETNAM FOREST CERTIFICATION OFFICE</h4>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-4 search-container">
                        <div class="search-block">
                            <div class="text-right hidden-xs international col"><a href="/Localization/Localization/SetLanguage?culture=vi-VN"><img alt="Vietnamese" src="~/themes/ChungChiRung/assets/VN-Vietnam-Flag-icon.png" />Vietnamese</a><a href="/Localization/Localization/SetLanguage?culture=en-US"> <img alt="United Kingdom" src="~/themes/ChungChiRung/assets/english.gif" />English</a> <a href="/">Visit PEFC International Website</a></div>
                            <div class="hidden-xs social-media-header"><a href="https://www.facebook.com/PEFCVN"><img src="~/themes/ChungChiRung/assets/facebook-icon-7f77aba2126d0fb4599d86986f555caf8f3aeb884df8359623bc82a1c97d3558.png" alt="Facebook icon" /></a> <a href="https://www.youtube.com/channel/UCRkKCVxYYs6T1DUXhAs--Yw/videos"><img src="~/themes/ChungChiRung/assets/youtube-icon.png" /></a> </div>
                            <div class="search-form-large hidden-xs">
                                <form class="form-inline" id="site-search-form" action="~/search" accept-charset="UTF-8" method="get">
                                    <input name="query" type="hidden"  /><label class="sr-only" for="query_"></label>
                                    <div class="input-group"><input type="text" name="Query" id="query" class="form-control" placeholder="@Localizer["Search here..."]" /><span class="input-group-btn"><button class="btn btn-default" name="commit" type="submit"><span class="glyphicon glyphicon-search"></span></button></span></div>
                                </form>
                            </div>
                            <div class="search-form-small visible-xs">
                                <form class="form-inline" id="site-search-form" action="search" accept-charset="UTF-8" method="get">
                                    <input name="utf8" type="hidden" value="" /><label class="sr-only" for="query_"></label>
                                    <div class="input-group"><input type="text" name="query" id="query" class="form-control" placeholder="Search..." /><span class="input-group-btn"><button class="btn btn-default" name="commit" type="submit"><span class="glyphicon glyphicon-search"></span></button></span></div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </header>
        </div>
        @await Component.InvokeAsync("CategoryMenu")
        <section id="page">
            <div class="row">
                <div class="col-xs-12">

                    @RenderSection("featured", false)
                    @RenderBody()
                </div>
            </div>
        </section>
        <footer id="footer">
            <div class="row three-column-content-block-widget">
                <div class="col-sm-4">
                    <h3>Contact Us</h3>
                    <p>Viet Nam Forest Certification Office<br /> Room 309, A9 Building<br />N° 2, Ngoc Ha Street. <br />   Ba Dinh district, Hanoi, Viet Nam. <br /> 
                    <p>Tel:  +84 384 387 92<br />Fax:  +84 384 387 93<br />E-mail: <a href="mailto:vanphongvfco@gmail.com" title="vanphongvfco@gmail.com">vanphongvfco@gmail.com</a></p>
                    <p><a href="disclaimer.html" title="Disclaimer">Disclaimer</a></p>
                    <p><a href="#" title="Privacy Policy">Privacy Policy</a></p>
                </div>
                <div class="col-sm-4">
                    @*<h3>The team</h3><p><strong>Dr. Le Van Binh</strong><br>Technical officer</p><p><strong>Msc. Khuat Thi Lan Anh</strong><br>Communications officer</p><p><strong>Mrs. Tran Thi Tuoi</strong><br>Accountant officer</p><p><strong>Dr. Nguyen Van Thịnh&nbsp;</strong><br>Volunteer technical officer</p><p><strong>Msc. Vo Trung Kiên&nbsp;</strong><br>Volunteer development officer</p>*@
                </div>
                <div class="col-sm-4">
                    <h3>Follow</h3>
                    <p>  <a href="https://www.facebook.com/PEFCVN"><img src="~/themes/ChungChiRung/assets/facebook-icon.png" /></a>   <a href="https://www.youtube.com/channel/UCRkKCVxYYs6T1DUXhAs--Yw/videos"><img src="~/themes/ChungChiRung/assets/youtube-icon.png" /></a> </p>
                </div>
            </div>
        </footer>
    </div>
    @RenderSection("scripts", required: false)
</body>
</html>
